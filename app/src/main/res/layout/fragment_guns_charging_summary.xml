<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center_horizontal"
    android:orientation="vertical"
    android:padding="@dimen/_10sdp"
    tools:context=".views.fragment.GunsLastChargingSummaryFragment">

    <include
        android:id="@+id/incHeader"
        layout="@layout/common_header" />

    <include
        android:id="@+id/incEVMacAddress"
        layout="@layout/common_summary_view"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_10sdp" />

    <include
        android:id="@+id/incChargingDuration"
        layout="@layout/common_summary_view"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <include
        android:id="@+id/incChargingStartDateTime"
        layout="@layout/common_summary_view"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <include
        android:id="@+id/incChargingEndDateTime"
        layout="@layout/common_summary_view"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <include
        android:id="@+id/incStartSOC"
        layout="@layout/common_summary_view"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <include
        android:id="@+id/incEndSOC"
        layout="@layout/common_summary_view"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <include
        android:id="@+id/incEnergyConsumption"
        layout="@layout/common_summary_view"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <include
        android:id="@+id/incSessionEndReason"
        layout="@layout/common_summary_view"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

</LinearLayout>